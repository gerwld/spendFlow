{
  "expo": {
    "name": "SpendFlow",
    "slug": "spendflow-spendings-tracker",
    "version": "0.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#000000",
      "image": "./assets/splash_image_light.png",
      "resizeMode": "contain",
      "dark": {
        "backgroundColor": "#000000",
        "image": "./assets/splash_image_light.png",
        "resizeMode": "contain"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.weblxapp.spendflow",
      "userInterfaceStyle": "automatic",
      "splash": {
        "backgroundColor": "#000000",
        "image": "./assets/splash_image_light.png",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#000000",
          "image": "./assets/splash_image_light.png",
          "resizeMode": "contain"
        }
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-icon-background.png",
        "backgroundColor": "#3bb7e7",
        "monochromeImage": "./assets/adaptive-icon.png"
      },
      "userInterfaceStyle": "automatic",
      "splash": {
        "backgroundColor": "#000000",
        "resizeMode": "contain",
        "image": "./assets/splash_image_light.png",
        "mdpi": "./assets/bootsplash/android/drawable-mdpi/splashscreen_image.png",
        "hdpi": "./assets/bootsplash/android/drawable-hdpi/splashscreen_image.png",
        "xhdpi": "./assets/bootsplash/android/drawable-xhdpi/splashscreen_image.png",
        "xxhdpi": "./assets/bootsplash/android/drawable-xxhdpi/splashscreen_image.png",
        "xxxhdpi": "./assets/bootsplash/android/drawable-xxxhdpi/splashscreen_image.png",
        "dark": {
          "backgroundColor": "#000000",
          "resizeMode": "contain",
          "image": "./assets/splash_image_light.png",
          "mdpi": "./assets/bootsplash/android/drawable-mdpi/splashscreen_image.png",
          "hdpi": "./assets/bootsplash/android/drawable-hdpi/splashscreen_image.png",
          "xhdpi": "./assets/bootsplash/android/drawable-xhdpi/splashscreen_image.png",
          "xxhdpi": "./assets/bootsplash/android/drawable-xxhdpi/splashscreen_image.png",
          "xxxhdpi": "./assets/bootsplash/android/drawable-xxxhdpi/splashscreen_image.png"
        }
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.weblxapp.spendflow"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "splash": {
        "backgroundColor": "#000000",
        "image": "./assets/splash_image_light.png",
        "resizeMode": "contain",
        "dark": {
          "backgroundColor": "#000000",
          "image": "./assets/splash_image_light.png",
          "resizeMode": "contain"
        }
      }
    },
    "plugins": [
      "expo-localization",
      "expo-router",
      "expo-asset"
    ]
  }
}
